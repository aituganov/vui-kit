{
  "name": "@v.aituganov/vui-kit",
  "version": "0.0.12",
  "description": "Gorgeous UI kit for vuejs 3",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/vui-kit.umd.cjs",
  "module": "./dist/vui-kit.js",
  "exports": {
    ".": {
      "import": "./dist/vui-kit.js",
      "require": "./dist/vui-kit.umd.cjs"
    }
