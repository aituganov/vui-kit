  "author": "Vladislav Aituganov",
  "license": "ISC",
  "dependencies": {
    "maska": "^2.1.8"
  },
  "peerDependencies": {
    "tailwindcss": "^3.3.2",
    "vee-validate": "^4.8.6",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.1",
    "vite": "^4.3.4",
    "vite-plugin-dts": "^2.3.0"
  }
